{
  "presets": [
    ["env", {
      "modules": false,
      "debug": true,
      "targets": {
        "node": "current",
        "browsers": ["last 3 versions"]
      }
    }],
    "react"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-object-rest-spread"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-constant-elements",
        "transform-react-inline-elements",
        "transform-react-remove-prop-types"
      ]
    }
  }
}
