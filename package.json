{
  "name": "tvg-react-boilerplate",
  "version": "1.0.0",
  "description": "Basic project to start other TVG frontend projects",
  "main": "src/index.js",
  "bin": {
    "server": "./bin/server",
    "server-dev": "./bin/server-dev"
  },
  "engines": {
    "node": ">=6.9.10"
  },
  "keywords": [
    "TVG",
    "frontend"
  ],
  "author": "Luis Couto <luis.couto@mindera.com>",
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "npm run build:dll",
    "test": "jest",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "./bin/eslint",
    "lint:report": "eslint --ext .js --ext .jsx -f stylish -f html -o reports/eslint/index.html .",
    "build": "run-s flow lint build:prod",
    "build:dll": "webpack --config config/webpack/dll.config.js --env.optimize=false",
    "build:dev": "webpack --config config/webpack/client.config.js --env.optimize=false",
    "build:prod": "cross-env NODE_ENV=production webpack --config config/webpack/client.config.js",
    "server:dev": "./bin/server-dev",
    "server:prod": "./bin/server",
    "start": "npm run start:prod",
    "start:dev": "./bin/server-dev",
    "start:prod": "run-s build:prod server:prod"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "tvg-conf": "git+ssh://git@bitbucket.org/betfair-us/tvg-conf.git#babel-preset-env",
    "tvg-mediator": "^0.6.0",
    "tvg-ui-bootstrap": "^1.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-preset-env": "^1.2.0",
    "babel-preset-react": "^6.23.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "circular-dependency-plugin": "^2.0.0",
    "cross-env": "^3.2.3",
    "css-loader": "^0.27.2",
    "eslint": "^3.17.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "flow-bin": "^0.41.0",
    "jest": "^19.0.2",
    "mkdirp": "^0.5.1",
    "node-mkdirp": "^0.0.1",
    "npm-run-all": "^4.0.2",
    "nyan-progress-webpack-plugin": "^1.1.4",
    "postcss-cssnext": "^2.9.0",
    "postcss-less-vars": "^1.2.5",
    "postcss-loader": "^1.3.3",
    "postcss-nested": "^1.0.0",
    "postcss-smart-import": "^0.6.10",
    "react-hot-loader": "next",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1"
  },
  "browserslist": [
    "last 3 versions"
  ]
}
